{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindStylesheet": "./src/index.css",
  "tailwindConfig": "./tailwind.config.js",
  "tailwindFunctions": ["clsx"],
  "tailwindPreserveWhitespace": true,
  "tailwindPreserveDuplicates": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always"
}
